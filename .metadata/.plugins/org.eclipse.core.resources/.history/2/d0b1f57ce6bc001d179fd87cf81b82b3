package pilhas;

public class PilhaInt {

	public final int N=6;
	int dados[] = new int[N];
	int topo;
	
	
	public void init(){
		topo = 0;
	}
	public void push(int elem){
	dados[topo] = elem;
	topo++;
	}
	public int pop(){
		topo--;
		return (dados[topo]);
	}
	public int top(){
	    topo--;
		return (dados[topo]);
	}
	
}
